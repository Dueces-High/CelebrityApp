<Question1>:
    BoxLayout:
        padding: 20
        spacing: 10
        orientation: "vertical"

        size: root.width, root.height

        orientation: "vertical"
        Image:
            id: llphoto
            source: "LLCOOLJ2.jpg"
            size_hint: 1, .5

        Button:
            markup: True
            text:"[size=25][b]Hint[/b][/size]"
            size_hint: .22, .06
            pos_hint: {"x": .38}
            on_release: root.hint1()
            on_press: root.hint_counter()


        Label:
            id: Clear
            multiline: False
            text:""
            size_hint: 1, .1


        BoxLayout:
            orientation: "horizontal"
            size_hint: 1, .1
            Button:
                text: "O"
                size_hint: (.1, 1)
                on_press: root.letterpress2("O")


            Button:
                text: "C"
                size_hint: (.1, 1)
                on_press: root.letterpress2("C")

            Button:
                text: "L"
                size_hint: (.1, 1)
                on_press: root.letterpress2("L")

            Button:
                text: "J"
                size_hint: (.1, 1)
                on_press: root.letterpress2("J")

            Button:
                text: "O"
                size_hint: (.1, 1)
                on_press: root.letterpress2("O")



            Button:
                text: "L"
                size_hint: (.1, 1)
                on_press: root.letterpress2("L")


            Button:
                text: "L"
                size_hint: (.1, 1)
                on_press: root.letterpress2("L")

        BoxLayout:
            orientation: "horizontal"
            size_hint: 1, .1

            Button:
                text: "Space"
                size_hint: 1, 1
                on_press: root.space(" ")
            Button:
                text: "Clear"
                size_hint: 1, 1
                on_press: root.clear()
        BoxLayout:
            orientation: "horizontal"
            size_hint: 1, .1
            Button:
                text: "Enter"
                size_hint: .75, 1
                on_press: root.questionpass()
        BoxLayout:
            orientation: "horizontal"
            size_hint: .45, .1
            pos_hint: { "x": .30}

            Label:
                text: "Hints Left:"
                size_hint: .25, 1

            Label:
                id: KivyLimit
                text: root.KivyLimitkv
                size_hint: .1, 1




